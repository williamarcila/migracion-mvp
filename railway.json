{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE"
  },
  "services": [
    {
      "name": "backend",
      "source": {
        "type": "path",
        "path": "./backend"
      },
      "dockerfilePath": "./backend/Dockerfile"
    },
    {
      "name": "frontend",
      "source": {
        "type": "path",
        "path": "./frontend"
      },
      "dockerfilePath": "./frontend/Dockerfile"
    }
  ]
}
